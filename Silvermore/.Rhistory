calcRollingAverages <- function(tickerData){
#VARIABLES
aggregate_sum <- 0
day <- 0
DAYS <- NULL
PRICES <- tickerData[, 2]
ROLLING_AVG <- NULL
#CALCULATION
for(price in PRICES){
aggregate_sum <- aggregate_sum + price
day <- day + 1
DAYS <- c(DAYS, day)
ROLLING_AVG <- c(ROLLING_AVG, aggregate_sum / day)
}
#RETURN
df <- data.frame(DAYS, PRICE, ROLLING_AVG)
result <- df
}
